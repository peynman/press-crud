{
  "name": "peynman/larapress-crud",
  "description": "API scaffolding with searching and filtering interfaces.",
  "type": "library",
  "license": "MIT",
  "version": "0.1",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Peyman Abdi",
      "email": "larapress@peynman.ir"
    }
  ],
  "autoload": {
    "psr-4": {
      "Larapress\\CRUD\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larapress\\CRUD\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larapress\\CRUD\\Providers\\PackageServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^7.3.0",
    "laravel/framework": "^8.0"
  },
  "require-dev": {
    "facade/ignition": "^2.3.6",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^5.0",
    "orchestra/testbench": "^6.2.0",
    "phpunit/phpunit": "^9.0",
    "phpmd/phpmd" : "@stable",
    "squizlabs/php_codesniffer": "3.*"
  }
}
